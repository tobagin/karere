#!/bin/bash
# Main executable script for Karere
# This file is generated by meson from karere.in

PYTHON="@PYTHON@"
INSTALLED_PYTHONPATH="@INSTALLED_PYTHONPATH@"


# Set up Python path for installed version
export PYTHONPATH="$INSTALLED_PYTHONPATH:$PYTHONPATH"


# Run the application
exec "$PYTHON" -m karere.main "$@"